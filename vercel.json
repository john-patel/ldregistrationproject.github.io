{
  "version": 2,
  "builds": [
    {
      "src": "LDBackend/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "LDBackend/wsgi.py"
    }
  ],
  "server": {
    "runtime": "vercel-python@0.24.0",
    "maxDuration": 30
  }
}
